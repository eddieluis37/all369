{
    "preset": "laravel",
    "rules": {
        "ordered_imports": {
            "sort_algorithm": "alpha"
        },
        "ordered_class_elements": {
            "order": [
                "use_trait",
                "case",
                "constant_public",
                "constant_protected",
                "constant_private",
                "property_public",
                "property_protected",
                "property_private",
                "construct",
                "destruct",
                "magic",
                "phpunit",
                "method_public",
                "method_protected",
                "method_private"
            ],
            "sort_algorithm": "none"
        },
        "binary_operator_spaces": {
            "operators": {
                "=>" : "align_single_space_minimal",
                "=" : "align_single_space_minimal"
            }
        }
    }
}